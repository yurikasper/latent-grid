{
  "name": "@yurikasper/latent-grid",
  "version": "0.1.3",
  "description": "Latent Grid TypeScript client",
  "author": "Yuri Kasper",
  "keywords": [
    "table extraction",
    "img2table"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yurikasper/latent-grid"
  },
  "type": "module",
  "publishConfig": {
