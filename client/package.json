    "check-types": "tsc --noEmit",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "0.89.2",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
